      "customFuncCfg" : { },
      "customQuery" : "SELECT\r\nmax(a.lastupdatedate) as lastupdatedate,\r\nmax(b.first_name || ' ' || b.last_name) as lastupdatedby,\r\na.recordnumber as recordnumber\r\nFROM (\r\nselect recordnumber, lastupdatedby, lastupdatedate from osc.t_osc_goldsheet_sso where to_date(lastupdatedate,'YYYY-MM-DD HH24:MI:SS') > sysdate - 1\r\nunion all\r\nselect recordnumber, lastupdatedby, lastupdatedate from osc.t_osc_goldsheet_fop_opty where to_date(lastupdatedate,'YYYY-MM-DD HH24:MI:SS') > sysdate - 1\r\nunion all\r\nselect recordnumber, lastupdatedby, lastupdatedate from osc.t_osc_goldsheet_fop_ss where to_date(lastupdatedate,'YYYY-MM-DD HH24:MI:SS') > sysdate - 1\r\nunion all\r\nselect recordnumber, lastupdatedby, lastupdatedate from osc.t_osc_goldsheet_fop_trend where to_date(lastupdatedate,'YYYY-MM-DD HH24:MI:SS') > sysdate - 1\r\nunion all\r\nselect recordnumber, lastupdatedby, lastupdatedate from osc.t_osc_goldsheet_rel_act where to_date(lastupdatedate,'YYYY-MM-DD HH24:MI:SS') > sysdate - 1\r\nunion all\r\nselect recordnumber, lastupdatedby, lastupdatedate from osc.t_osc_goldsheet_rel_goal where to_date(lastupdatedate,'YYYY-MM-DD HH24:MI:SS') > sysdate - 1\r\nunion all\r\nselect recordnumber, lastupdatedby, lastupdatedate from osc.t_osc_goldsheet_str_player where to_date(lastupdatedate,'YYYY-MM-DD HH24:MI:SS') > sysdate - 1\r\nunion all\r\nselect goldsheetrecordnumber as recordnumber, lastupdatedby, lastupdateddate as lastupdatedate from osc.t_osc_mh_activity where goldsheetrecordnumber is not null and to_date(lastupdateddate,'YYYY-MM-DD HH24:MI:SS') > sysdate - 1\r\n) a, osc.t_osc_resource b\r\nWHERE\r\na.lastupdatedby = b.username\r\nGROUP BY a.recordnumber",
